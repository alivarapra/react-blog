{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://blog.hust.cf",
  "dependencies": {
    "highlight.js": "^9.12.0",
    "marked": "^0.3.19",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.79.0"
  },
  "scripts": {
    "metadata": "node ./public/_posts/service.js",
    "start": "npm run metadata && react-scripts start",
    "build": "npm run metadata && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "watch": "./node_modules/npm-watch/cli.js"
  },
  "watch": {
    "build": {
      "patterns": "src",
      "extensions": "js, css"
    }
  },
  "devDependencies": {
    "npm-watch": "^0.3.0",
    "yaml-front-matter": "^4.0.0"
  },
  "engines" : {
    "node" : ">=9.x"
  }
}
